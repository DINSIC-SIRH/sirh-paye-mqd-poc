<xsd:schema elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0" id="siri_types" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:core="http://www.govtalk.gov.uk/core">
  <xsd:annotation>
    <xsd:appinfo xmlns:gms="http://www.govtalk.gov.uk/CM/gms">
